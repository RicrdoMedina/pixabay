{
	"name": "arquitertura-hexagonal",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "jest --verbose",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage --coverageReporters=json-summary,text,lcov",
		"test:report": "jest --coverage --reporters=jest-html-reporters && npx playwright test --reporter=html",
		"lint": "next lint",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"analyze": "cross-env ANALYZE=true next build",
		"prebuild": "rimraf .next && rimraf dist",
		"report:tests": "npx playwright show-report && open reports/test.v2.html",
		"report:logs": "cat reports/test.v2.html",
		"e2e": "npx playwright test",
		"e2e:debug": "npx playwright test --debug",
		"e2e:headed": "npx playwright test --headed",
		"e2e:report": "npx playwright show-report",
		"e2e:update": "npx playwright test --update-snapshots",
		"e2e:install": "npx playwright install",
		"e2e:ci": "npx playwright test --reporter=dot"
	},
	"dependencies": {
		"@fontsource/open-sans": "^5.1.1",
		"@heroicons/react": "^2.2.0",
		"@reduxjs/toolkit": "^2.5.1",
		"axios": "^1.7.9",
		"next": "15.1.6",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-multi-carousel": "^2.8.5",
		"react-redux": "^9.2.0",
		"swiper": "^11.2.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@playwright/test": "^1.50.1",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^29.5.14",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"babel-jest": "^29.7.0",
		"eslint": "^9",
		"eslint-config-next": "15.1.6",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-filenames": "^1.3.2",
		"eslint-plugin-prettier": "^5.2.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-extended": "^4.0.2",
		"jest-html-reporter": "^4.0.1",
		"jest-html-reporters": "^3.1.7",
		"postcss": "^8",
		"prettier": "^3.4.2",
		"tailwindcss": "^3.4.1",
		"ts-jest": "^29.2.5",
		"typescript": "^5"
	}
}
